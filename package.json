{
  "name": "@splytech-io/currency",
  "version": "0.1.2",
  "main": "lib/currency.js",
  "scripts": {
    "build": "tsc -d",
    "pretest": "rm -f lib/*.d.ts lib/*.js",
    "prepublish": "npm run build",
    "test": "nyc mocha --require ts-node/register 'lib/*.spec.ts'"
  },
  "author": "Andrius Skerla",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "chai": "^4.1.2",
    "mocha": "^3.5.3",
    "nyc": "^11.4.1",
    "ts-mocha": "^1.0.3",
    "ts-node": "^5.0.0",
    "typescript": "^2.6.2"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/splytech-io/node-currency.git"
  },
  "bugs": {
    "url": "https://github.com/splytech-io/node-currency/issues"
  },
  "homepage": "https://github.com/splytech-io/node-currency#readme",
  "description": "currency",
  "keywords": [
    "currency"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "include": [
      "lib"
    ],
    "exclude": [
      "lib/*.spec.ts",
      "lib/*.d.ts"
    ]
  }
